{
  "compilerOptions": {
    "target": "ES6",
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "pretty": false,
    "removeComments": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "sourceMap": true,
    "outFile": "./dist/archetypum.js"
  },
  "files": [
      "./src/archetypum.ts"
  ]
}
