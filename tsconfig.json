{
  "compilerOptions": {
    "target": "es6",
    "module": "system",
    "moduleResolution": "node",
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "pretty": true,
    "removeComments": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "sourceRoot": "./src/",
    "outFile": "./dist/archetypum.js",
    "sourceMap": true
  }
}
